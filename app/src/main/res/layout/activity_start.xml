<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/common_background"
    tools:context="com.example.administrator.divination.activity.StartActivity">
    <LinearLayout
        android:id="@+id/start_appTitle"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:orientation="horizontal"
        android:background="@drawable/title_background"
        >
        <ImageView
            android:id="@+id/start_title_back"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/my_back"
            />
        <ImageView
            android:id="@+id/dreamContent_title_img"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="wrap_content"
            android:src="@drawable/title_all_star"/>

    </LinearLayout>

    <GridView
        android:id="@+id/start_gridView"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:layout_below="@id/start_appTitle"
        android:numColumns="2"/>
</RelativeLayout>
